{
    "name": "glucnac/datetimeinterval",
    "description": "DateTimeInterval is wrapper of DateInterval that provides useful methods for calculating delays.",
    "type": "library",
    "keywords": ["DateTime","Date","Time","Interval","DateTime operation","date_diff"],
    "homepage": "https://github.com/GlucNAc/DateTimeInterval",
    "license": "MIT",
    "authors": [
        {
            "name": "GlucNAc",
            "homepage": "https://github.com/GlucNAc"
        }
    ],
    "support": {
        "issues": "https://github.com/GlucNAc/DateTimeInterval/-/issues",
        "source": "https://github.com/GlucNAc/DateTimeInterval"
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^9.5",
        "symplify/easy-coding-standard": "^12.1"
    },
    "autoload": {
        "psr-4": {
            "GlucNAc\\DateTimeInterval\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GlucNAc\\DateTimeInterval\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
